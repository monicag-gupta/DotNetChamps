formsEg.html

<html>
    <head>
        <title>Forms Eg</title>
    </head>
    <body>
        <form action="first.html" method="post">
            <label for="nameId">Username: </label>
            <input type="text" name="a" id="nameId"
            placeholder="Enter Name:" required  
            autocomplete="given-name">
            <br/>
            <label for="passId">Password: </label>
            <input type="password" name="a" id="passId"
            placeholder="Enter Password:" required>
            <br/>
            Email: <input type="email"><br/>
            url: <input type="url"><br/>
            Age: <input type="number" min="0" max="150"><br/>
            <select>
                <option>C#</option>
                <optgroup label="C Family">
                    <option>C++</option><option>C</option>
                </optgroup>
                <option>Java</option>
                <optgroup label="Web Family">
                    <option>HTML</option><option>CSS</option>
                </optgroup>
                <option>Asp.Net</option>
            </select> <br/>
            <input type="submit">
        </form>
    </body>
</html>




<html>
    <head><title>JS Eg</title></head>
    <body>
        x: <input type="text" id="x"/><br/>
        y: <input type="text" id="y"/><br/>
        <button onclick="add();">+</button> 
        <button onclick="sub();">-</button>
        <h3>Result: <div id="result"></div></h3>
        <script type="text/javascript">
            function add(){
                var a=document.getElementById("x").value;
                var b=document.getElementById("y").value;
                var res = parseInt(a) + parseInt(b);
                document.getElementById("result").innerHTML=res;
            }
            function sub(){
                var a=document.getElementById("x").value;
                var b=document.getElementById("y").value;
                var res = parseInt(a) - parseInt(b);
                document.getElementById("result").innerHTML=res;
            }
        </script>
    </body>
</html>
